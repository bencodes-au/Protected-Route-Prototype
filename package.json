{
  "name": "fullstack-app",
  "private": true,
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "nodemon backend/server.js",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "vite build",
    "test": "vitest run",
    "docker:build:backend": "docker build -f Dockerfile.backend -t protected-route/backend:dev ./backend",
    "docker:build:frontend": "docker build -f Dockerfile.frontend -t protected-route/frontend:dev ./frontend",
    "docker:build:all": "npm run docker:build:backend && npm run docker:build:frontend"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/ui": "^3.1.4",
    "concurrently": "^8.2.0",
    "jsdom": "^26.1.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.1",
    "vite": "^6.3.1",
    "vitest": "^3.1.4"
  }
}
